    "@nuxt/ui": "^3.1.1",
    "@pinia/nuxt": "^0.11.0",
    "@tailwindcss/vite": "^4.1.5",
    "@tanstack/vue-query": "^5.76.0",
    "@unhead/vue": "^2.0.8",
    "argon2": "^0.43.0",
